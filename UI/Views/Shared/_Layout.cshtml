<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Our Facelook Application</title>
    <link href="~/Content/LayoutCSS.css" rel="stylesheet" />
    <link rel="SHORTCUT ICON" href="data:image/png;base64," />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" id="navbarID">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("Facelook", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" ,id = "FacelookApplicationID" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home", new { @id = "MenuNavbarID" })</li>
                    <li>@Html.ActionLink("About", "About", "Home", new { @id = "MenuNavbarID" })</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home", new { @id = "MenuNavbarID" })</li>
                </ul>
                @Html.Partial("_LoginPartial", new { @id = "MenuNavbarID" })
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
           
            <p>&copy; 2018 - Our Facelook Application - Develop by <mark>Shay Shabtay</mark>  & <mark>Ariel Halevi</mark> & <mark>Omer Carmeli</mark>  </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
@{ 
    UI.SignalR.SignalRClient client = new UI.SignalR.SignalRClient();
    client.Login("CHECK");
}